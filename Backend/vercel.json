{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/fac(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/api/stu(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/api/ass(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/api/notice(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/api/att(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "PORT": "3000",
      "URI": "your-database-uri"
    }
  }
  